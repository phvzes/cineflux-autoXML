
# Example Environment Variables Template
# Copy this file to .env.local to override settings

# Node environment (development/production)
NODE_ENV=development

# Application version
VITE_APP_VERSION=0.1.0

# WebAssembly CDN URL
# Leave empty for local files or set to CDN URL for production
VITE_WASM_CDN_URL=

# Feature flags (JSON string of enabled features)
# Format: {"featureName":true/false,"anotherFeature":true/false}
VITE_FEATURE_FLAGS={"debugMode":false,"experimentalFeatures":false,"advancedAnalysis":true}
